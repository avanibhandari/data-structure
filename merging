#include <iostream>
using namespace std;
int main()
{
  int a[10],b[10],i,j,n,p,s[10],m,k;
  cout<<"enter size of array 1";
  cin>>n;
  cout<<"enter elements in array";
  for(i=0;i<n;i++)
  {
    cin>>a[i];
  }
cout<<"enter size of array 2 ";
cin>>p;
cout<<"enter elements in array";
for(i=0;i<p;i++)
{
  cin>>b[i];
}
for(i=0;i<n;i++)
{
s[i]=a[i];
}
m=n+p;
for(i=0;i<n;i++)
{
  for(j=n;j<m;j++)
  {s[j]=b[i];
  }}
  //adding done output
  for(i=0;i<m;i++)
  {
    cout<<s[i] ;
     }
     return 0;
}
